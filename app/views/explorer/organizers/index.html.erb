<% provide(:title , 'Organizers' ) %>

<div class="page">
	<h2 class="page-header">Participating Organizers</h2>
	<% @organizers.each do |o| %>
		<%= link_to organizer_path(o) do %>
			<div class="col-md-3 text-center">
				<%= image_tag o.logo, :class=>'img-responsive' %>
				<p class="lead"><%= o.name %></p>
			</div>
		<% end %>
	<% end %>
</div>